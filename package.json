{
  "name": "shuffleplus",
  "version": "1.0.0",
  "description": "Merge Spotify playlists dynamically",
  "main": "none",
  "dependencies": {
    "concurrently": "^5.3.0",
    "spotify-web-api-node": "^5.0.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {},
  "scripts": {
    "install-all": "npm install . ./client ./server",
    "production-client": "cd client && npm run production",
    "production-server": "cd server && npm run production",
    "production": "concurrently \"npm:production-*\"",
    "dev-client": "cd client && npm run dev",
    "dev-server": "cd server && npm run dev",
    "dev": "concurrently \"npm:dev-*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ophunt/ShufflePlus.git"
  },
